  "module": "app/index.js",
  "scripts": {
    "start": "cross-env HOST=0.0.0.0 PORT=5058 PUBLIC_PATH=http://127.0.0.1:5058/ yarn run dev",
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config webpack.config.js --process --color --watch",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.js --process --color --bail --info-verbosity verbose",
    "test": "BABEL_ENV=test jest --config jest.config.json",
    "test:watch": "BABEL_ENV=test jest --config jest.config.json --watch",
    "lint": "lint-staged",
    "lint-fix": "eslint -c .eslintrc.js --cache --cache-location=.cache/.eslint --fix -- app && ./node_modules/.bin/tslint --project tsconfig.json --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "lint-staged": {
    "(app)/*.{js,jsx,ts,tsx,md,css,vue}": [
      "prettier --write",
      "git add"
    ],
    "(app)/*.scss": [
      "prettier --write",
      "git add"
    ],
    "(app)/*.{js,jsx,vue}": [
      "eslint --fix",
      "git add"
    ],
    "(app)/*.{ts,tsx}": [
      "./node_modules/.bin/tslint --project tsconfig.json --fix",
      "git add"
    ]
  },
